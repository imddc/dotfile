    },
    "PowerShell": {
      "icon": "terminal-powershell",
      "source": "PowerShell"
    },
    "Windows PowerShell": {
      "path": "C:\\windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
    }
  },

  /// scm
  "scm.diffDecorationsGutterWidth": 2,
  "scm.inputFontSize": 14,

  /// debug
